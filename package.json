{
  "name": "portfolio-v2",
  "description": "The dev portfolio of Melanie Seltzer, built using GatsbyJS",
  "version": "2.0.0",
  "author": "Melanie Seltzer <melleh11@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/melanieseltzer/portfolio-v2.git"
  },
  "license": "MIT",
  "keywords": [
    "gatsby",
    "react"
  ],
  "scripts": {
    "build": "gatsby build",
    "start": "gatsby develop",
    "lint": "tslint -t verbose -c tslint.json 'src/**/*.{ts,tsx}'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "tslint --fix",
      "prettier --write",
      "git add"
    ],
    "*.{js,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "@types/react-helmet": "^5.0.7",
    "gatsby": "^2.0.76",
    "gatsby-image": "^2.0.20",
    "gatsby-plugin-manifest": "^2.0.9",
    "gatsby-plugin-offline": "^2.0.16",
    "gatsby-plugin-react-helmet": "^3.0.2",
    "gatsby-plugin-sharp": "^2.0.14",
    "gatsby-plugin-typescript": "^2.0.3",
    "gatsby-source-filesystem": "^2.0.8",
    "gatsby-transformer-sharp": "^2.1.8",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-helmet": "^5.2.0",
    "styled-components": "^4.1.3",
    "typescript": "^3.1.1"
  },
  "devDependencies": {
    "gatsby-plugin-tslint": "^0.0.2",
    "husky": "^1.2.0",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3",
    "tslint": "^5.12.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-loader": "^3.5.4",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^3.6.0"
  }
}
