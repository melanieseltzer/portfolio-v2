{
  "name": "portfolio-v2",
  "description": "The dev portfolio of Melanie Seltzer, built using GatsbyJS",
  "version": "2.0.0",
  "author": "Melanie Seltzer <melleh11@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/melanieseltzer/portfolio-v2.git"
  },
  "license": "MIT",
  "keywords": [
    "gatsby",
    "react"
  ],
  "scripts": {
    "build": "gatsby build",
    "start": "gatsby develop",
    "eslint": "eslint 'src/**/*.{js,ts,jsx,tsx}'",
    "tslint": "tslint -t verbose -c tslint.json --project tsconfig.json 'src/**/*.{ts,tsx}'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "*.{ts,tsx}": [
      "eslint --fix",
      "tslint --fix",
      "prettier --write",
      "git add"
    ],
    "*.{json}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "@types/react-helmet": "^5.0.8",
    "@types/styled-components": "^4.1.5",
    "gatsby": "^2.0.91",
    "gatsby-image": "^2.0.26",
    "gatsby-plugin-manifest": "^2.0.9",
    "gatsby-plugin-offline": "^2.0.21",
    "gatsby-plugin-react-helmet": "^3.0.2",
    "gatsby-plugin-sharp": "^2.0.14",
    "gatsby-plugin-styled-components": "^3.0.4",
    "gatsby-plugin-typescript": "^2.0.3",
    "gatsby-plugin-web-font-loader": "^1.0.4",
    "gatsby-source-filesystem": "^2.0.13",
    "gatsby-source-graphql": "^2.0.8",
    "gatsby-source-medium": "^2.0.2",
    "gatsby-transformer-sharp": "^2.1.10",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-helmet": "^5.2.0",
    "react-icons": "^3.3.0",
    "react-markdown": "^4.0.6",
    "styled-components": "^4.1.3",
    "typescript": "^3.1.1"
  },
  "devDependencies": {
    "eslint": "^5.12.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.3",
    "eslint-plugin-typescript": "^0.14.0",
    "gatsby-plugin-tslint": "^0.0.2",
    "husky": "^1.2.0",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3",
    "tslint": "^5.12.1",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.17.0",
    "tslint-loader": "^3.5.4",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^3.6.0",
    "tslint-react-a11y": "^1.0.0",
    "typescript-eslint-parser": "^21.0.2"
  }
}
